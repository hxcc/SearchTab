{
  "manifest_version": 3,
  "name": "Search Tab",
  "version": "3.0",
  "description": "仿谷歌首页的新标签页，集成多种搜索引擎和壁纸自定义功能",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "chrome_url_overrides": {
    "newtab": "newtab.html"
  },
  "permissions": [
    "storage",
    "bookmarks"
  ],
  "web_accessible_resources": [
    {
      "resources": ["default-wallpaper.jpg"],
      "matches": ["<all_urls>"]
    }
  ]
}